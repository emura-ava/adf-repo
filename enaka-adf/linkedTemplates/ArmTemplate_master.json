{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "enaka-adf"
		},
		"pipeline1_properties_variables_testVariable1": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"defaultValue": "testVariable1"
			}
		},
		"pipeline2_properties_variables_testVariable2": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"defaultValue": "testVariable2"
			}
		},
		"default_properties_testGlobal1": {
			"type": "string"
		},
		"default_properties_testGlobal2_value": {
			"type": "string",
			"defaultValue": "replaceglobal2"
		},
		"default_properties_testGlobal3": {
			"type": "string"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "enaka-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"default_properties_testGlobal1": {
						"value": "[parameters('default_properties_testGlobal1')]"
					},
					"default_properties_testGlobal2_value": {
						"value": "[parameters('default_properties_testGlobal2_value')]"
					},
					"default_properties_testGlobal3": {
						"value": "[parameters('default_properties_testGlobal3')]"
					},
					"pipeline2_properties_variables_testVariable2": {
						"value": "[parameters('pipeline2_properties_variables_testVariable2')]"
					},
					"pipeline1_properties_variables_testVariable1": {
						"value": "[parameters('pipeline1_properties_variables_testVariable1')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}