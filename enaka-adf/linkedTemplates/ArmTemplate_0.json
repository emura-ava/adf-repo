{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "enaka-adf"
		},
		"default_properties_testGlobal1": {
			"type": "string"
		},
		"default_properties_testGlobal2_value": {
			"type": "string",
			"defaultValue": "replaceglobal2"
		},
		"default_properties_testGlobal3": {
			"type": "string"
		},
		"pipeline2_properties_0_type": {
			"type": "string",
			"defaultValue": "SetVariable"
		},
		"pipeline2_properties_parameters_testParameter2": {
			"type": "object",
			"defaultValue": {
				"type": "string"
			}
		},
		"pipeline1_properties_0_type": {
			"type": "string",
			"defaultValue": "ExecutePipeline"
		},
		"pipeline1_properties_parameters_testParameter1": {
			"type": "object",
			"defaultValue": {
				"type": "string",
				"defaultValue": "testParameter1"
			}
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/default')]",
			"type": "Microsoft.DataFactory/factories/globalparameters",
			"apiVersion": "2018-06-01",
			"properties": {
				"testGlobal1": {
					"type": "string",
					"value": "[parameters('default_properties_testGlobal1')]"
				},
				"testGlobal2": {
					"type": "string",
					"value": "[parameters('default_properties_testGlobal2_value')]"
				},
				"testGlobal3": {
					"type": "string",
					"value": "[parameters('default_properties_testGlobal3')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline2')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Set variable1",
						"type": "[parameters('pipeline2_properties_0_type')]",
						"dependsOn": [],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "testVariable2",
							"value": "rtesttest"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"testParameter2": "[parameters('pipeline2_properties_parameters_testParameter2')]"
				},
				"variables": {
					"testVariable2": {
						"type": "String",
						"defaultValue": "testVariable2"
					}
				},
				"annotations": [],
				"lastPublishTime": "2024-03-04T15:45:24Z"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Execute Pipeline1",
						"type": "[parameters('pipeline1_properties_0_type')]",
						"dependsOn": [],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "pipeline2",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {
								"testParameter2": {
									"value": "@variables('testVariable1')",
									"type": "Expression"
								}
							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"testParameter1": "[parameters('pipeline1_properties_parameters_testParameter1')]"
				},
				"variables": {
					"testVariable1": {
						"type": "String",
						"defaultValue": "testVariable1"
					}
				},
				"annotations": [],
				"lastPublishTime": "2024-03-04T15:45:24Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/pipeline2')]"
			]
		}
	]
}